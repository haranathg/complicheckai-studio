# Backend API URL
VITE_API_URL=http://localhost:8000

# Parser Options - set to 'true' to enable each parser in the dropdown
# Landing AI is always enabled
VITE_ENABLE_CLAUDE_VISION=true
VITE_ENABLE_GEMINI_VISION=true
VITE_ENABLE_BEDROCK_CLAUDE=true

# AWS Cognito Authentication
# Set VITE_AUTH_DISABLED=true to skip authentication for local development
VITE_AUTH_DISABLED=true
# Cognito User Pool ID (from Terraform output: user_pool_id)
VITE_COGNITO_USER_POOL_ID=
# Cognito App Client ID (from Terraform output: client_id)
VITE_COGNITO_CLIENT_ID=
# Cognito Region (default: ap-southeast-2)
VITE_COGNITO_REGION=ap-southeast-2
